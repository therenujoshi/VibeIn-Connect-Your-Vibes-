nect',{\r\n            method : \"put\",\r\n            headers : {\r\n            \"Content-Type\" : \"application/json\",\r\n            \"Authorization\" : \"Bearer \"+ localStorage.getItem(\"jwt\")\r\n        },\r\n        body:JSON.stringify({\r\n            connectId: userid\r\n        })\r\n        }).then(res=>res.json())\r\n        .then(data=>{\r\n            dispatch({type:\"UPDATE\", payload:{following:data.following, connectors:data.connectors}})\r\n            localStorage.setItem(\"user\", JSON.stringify(data));\r\n            setProfile((prevState)=>{\r\n                return {\r\n                    ...prevState,\r\n                    user: {\r\n                        ...prevState.user,\r\n                        connectors:[...prevState.user.connectors, data._id]\r\n                    }\r\n                }\r\n            });\r\n            setShowConnect(false)\r\n        })\r\n    }\r\n\r\n    //unfollow user\r\n    const disconnectUser = ()=>{\r\n        fetch('/disconnect',{\r\n            method : \"put\",\r\n            headers : {\r\n            \"Content-Type\" : \"application/json\",\r\n            \"Authorization\" : \"Bearer \"+ localStorage.getItem(\"jwt\")\r\n        },\r\n        body:JSON.stringify({\r\n            disconnectId: userid\r\n        })\r\n        }).then(res=>res.json())\r\n        .then(data=>{\r\n\r\n            dispatch({type:\"UPDATE\", payload:{following:data.following, connectors:data.connectors}})\r\n            localStorage.setItem(\"user\", JSON.stringify(data));\r\n           \r\n            setProfile((prevState)=>{\r\n                const newConnector = prevState.user.connectors.filter(item=>item!= data._id)\r\n                return {\r\n                    ...prevState,\r\n                    user: {\r\n                        ...prevState.user,\r\n                        connectors:newConnector\r\n                    }\r\n                }\r\n            });\r\n            setShowConnect(true);\r\n  